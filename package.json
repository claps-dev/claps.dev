{
  "name": "claps.dev",
  "version": "0.1.0",
  "description": "Help you funding the creators and projects you appreciate with crypto currencies.",
  "repository": "git@github.com:JounQin/Claps.dev.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:app": "env-cmd nuxt-ts build",
    "build:server": "cross-env NODE_ENV=production r -i server/index -f cjs -x '*' -o dist",
    "dev": "env-cmd run-p dev:*",
    "dev:router": "tsnd -O '{\"module\":\"commonjs\"}' -T server/router/dev",
    "dev:server": "tsnd -O '{\"module\":\"commonjs\"}' -T server",
    "lint": "run-p lint:*",
    "lint:es": "env-cmd eslint . --cache --ext md,ts,tsx,vue",
    "lint:style": "stylelint --cache 'src/**/*.{scss,vue}'",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "start": "cross-env NODE_ENV=production env-cmd node dist/cjs.min"
  },
  "dependencies": {
    "@mdi/js": "^4.7.95",
    "@octokit/rest": "^16.36.0",
    "@rxts/koa-proxy": "^0.2.0",
    "@rxts/koa-router-decorators": "^0.1.5",
    "@vue/composition-api": "^0.3.4",
    "axios": "^0.19.1",
    "core-js": "^3.6.3",
    "koa": "^2.11.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "lodash-es": "^4.17.15",
    "mathjs": "^6.5.0",
    "mixin-node-sdk": "^0.1.4",
    "nuxt": "^2.11.0",
    "qrcode": "^1.4.4",
    "qs": "^6.9.1",
    "uuid": "^3.3.3",
    "vue": "^2.6.11",
    "vue-qrcode": "^0.2.2",
    "vue-router": "^3.1.3",
    "vuetify": "^2.2.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.1.17",
    "@d-ts/vue": "^0.3.0",
    "@nuxt/types": "^0.6.0",
    "@nuxt/typescript-build": "^0.5.5",
    "@nuxt/typescript-runtime": "^0.3.6",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@pkgr/rollup": "^0.8.10",
    "@types/http-proxy": "^1.17.3",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa-session": "^5.10.1",
    "@types/lodash": "^4.14.149",
    "@types/mathjs": "^6.0.3",
    "@types/qs": "^6.9.0",
    "@types/uuid": "^3.4.6",
    "babel-preset-vca-jsx": "^0.3.4",
    "consola": "^2.11.3",
    "cpy-cli": "^3.0.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "eslint-config-vuetify": "^0.4.1",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.1",
    "style-resources-loader": "^1.3.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4",
    "yarn-deduplicate": "^1.1.1"
  },
  "resolutions": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/node": "^13.1.6"
  },
  "browserslist": [
    "extends @1stg/browserslist-config/modern"
  ],
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "eslintConfig": {
    "extends": "@1stg/eslint-config/loose",
    "overrides": [
      {
        "files": "*.{vue,ts,tsx}",
        "extends": [
          "@nuxtjs/eslint-config-typescript",
          "vuetify",
          "@1stg/eslint-config/loose"
        ]
      }
    ]
  },
  "postcss": {
    "extends": "@1stg"
  },
  "prettier": "@1stg/prettier-config",
  "remarkConfig": {
    "plugins": [
      "@1stg/remark-config"
    ]
  },
  "renovate": {
    "extends": "@1stg"
  },
  "stylelint": {
    "extends": [
      "@1stg/stylelint-config/scss/loose",
      "@1stg/stylelint-config/modules"
    ]
  }
}

{
  "extends": "@1stg/tsconfig/app",
  "compilerOptions": {
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["./*"],
      "lodash": ["lodash-es"],
      "vuetify": ["vuetify/src"]
    },
    "target": "esnext",
    "types": ["@d-ts/vue", "@nuxt/vue-app", "@types/node"]
  }
}
